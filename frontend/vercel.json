{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/player/(.*)",
      "dest": "https://premier-zone.onrender.com/api/v1/player/$1"
    }
  ]
}
